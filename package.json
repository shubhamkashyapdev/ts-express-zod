{
  "name": "typescript-express-starter",
  "version": "1.0.0",
  "description": "Nodejs setup with typescript and express.js",
  "main": "build/app.js",
  "scripts": {
    "start": "cross-env NODE_ENV=PRODUCTION npm run build && node build/main.js",
    "dev": "cross-env NODE_ENV=DEVELOPMENT ts-node-dev -r tsconfig-paths/register --transpile-only --poll src/main.ts",
    "test": "cross-env NODE_ENV=TESTING jest",
    "test:watch": "cross-env NODE_ENV=TESTING jest --watchAll --detectOpenHandles",
    "build": "tsc --project tsconfig.json && tscpaths -p tsconfig.json -s ./src -o ./build"
  },
  "author": "Utkarsh Siddharth",
  "keywords": [],
  "license": "ISC",
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/body-parser": "^1.19.2",
    "@types/config": "^0.0.41",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.1",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/lodash": "^4.14.182",
    "@types/morgan": "^1.9.3",
    "@types/nanoid": "^3.0.0",
    "@types/node": "^17.0.40",
    "@types/oauth": "^0.9.1",
    "@types/response-time": "^2.3.5",
    "@types/supertest": "^2.0.12",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "colors": "^1.4.0",
    "concurrently": "^7.2.1",
    "eslint": "^8.17.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jest": "^26.5.1",
    "jest": "^28.1.1",
    "jest-circus": "^29.5.0",
    "mongodb-memory-server": "^8.6.0",
    "nodemon": "^2.0.16",
    "prettier": "^2.6.2",
    "supertest": "^6.2.3",
    "ts-jest": "^28.0.4",
    "ts-node": "^10.8.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.1.2",
    "tscpaths": "^0.0.9",
    "typescript": "^4.7.3"
