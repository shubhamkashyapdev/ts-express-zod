    "dotenv": "^16.3.1",
    "express": "^4.18.1",
    "express-async-handler": "^1.2.0",
    "helmet": "^5.1.0",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^8.5.1",
    "langchain": "^0.0.98",
    "lodash": "^4.17.21",
    "mongoose": "^6.3.5",
